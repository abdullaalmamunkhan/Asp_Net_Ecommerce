@using System.Security.Claims;
@{

    var loginUserFullName = (User.Claims.FirstOrDefault(c => c.Type.ToString() == ClaimTypes.GivenName) != null) ? User.Claims.FirstOrDefault(c => c.Type.ToString() == ClaimTypes.GivenName).Value : "";
    var loginUserEmail = (User.Claims.FirstOrDefault(c => c.Type.ToString() == ClaimTypes.Email) != null) ? User.Claims.FirstOrDefault(c => c.Type.ToString() == ClaimTypes.Email).Value : "";
    var loginUserRole = (User.Claims.FirstOrDefault(c => c.Type.ToString() == ClaimTypes.Role) != null) ? User.Claims.FirstOrDefault(c => c.Type.ToString() == ClaimTypes.Role).Value : "";
    var loginUserId = 11;

}
