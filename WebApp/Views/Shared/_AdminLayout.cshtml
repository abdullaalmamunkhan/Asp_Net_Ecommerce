    <script type="text/javascript">
        $(document).ready(function () {

            $(document).ajaxStart(function () {
                $('.dummy_ajax_loader').show();
            }).ajaxStop(function () {
                $('.dummy_ajax_loader').hide();
            });


            var successMeessage = "@TempData["InfoMessage"]";
            var errorMessage = "@TempData["ErrorMessage"]";
            if (successMeessage && successMeessage !="") {
                ShowNotifyMessage(successMeessage, "success");
            }
            if (errorMessage && errorMessage !="") {
                ShowNotifyMessage(errorMessage, "error");
            }
        });
